func say_apple() { } 		// 0
func dont_say_apple() {} 	// 2

func main() {
	if 1234523 == 0 && 0 == 1 {
		say_apple();
	}
	
	if 1 == 1 {
		dont_say_apple();
	}

	if 1 == 1 || 2 == 0 {
		say_apple();
	}

	if 1 == 1 && 2 == 2 {
		dont_say_apple();
	}

	if 1 == 1 && 2 == 0 {
		dont_say_apple();
	}

	say_apple();
	dont_say_apple();
}