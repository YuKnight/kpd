type Dimension struct {
	width us32,
	height us32,
};

type Game_Window struct {
	title string,
	x us32,
	y us32,
	size Dimension,
};

func (gw *Game_Window) change_title(new_title string) {
	gw.title = new_title;
}

func main() {
	let window *Game_Window;
	window.title = "Tetris";
	window.x = window.y = 0;
	window.size.width = 1280;
	window.size.height = 720;

	doesnt_exist();
	window.doesnt_exist();
	window.change_title("my new title!");
}