func writeln(msg string) {}

type Animal trait {
	speak func() void,
};

type Cow struct {};

func (c Cow) speak() {
	writeln("hi im a cow");
}

type Frog struct {};

func (f Frog) speak() {
	writeln("m name jeff");
}

func speak!(T: Animal)(animal T) {
	// todo this does not resolve
	// params are not introduced into the symbol table?
	animal.speak();
}

func main() {
	let bobby Cow;
	speak!(Cow)(cow);

	let jeff Frog;
	speak!Frog(frog);
}